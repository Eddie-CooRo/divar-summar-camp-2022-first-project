    <script>
      const url_string = window.location.href;
      const url = new URL(url_string);
      const token = url.searchParams.get("token");
      const getInfo = async (token) => {
        const data = await fetch(
          `https://api.divar.ir/v8/posts-v2/web/${token}`,
          {
            headers: {
              accept: "application/json-filled",
              "accept-language": "en-US,en;q=0.9",
              "sec-ch-ua":
                '".Not/A)Brand";v="99", "Google Chrome";v="103", "Chromium";v="103"',
              "sec-ch-ua-mobile": "?0",
              "sec-ch-ua-platform": '"Linux"',
              "sec-fetch-dest": "empty",
              "sec-fetch-mode": "cors",
              "sec-fetch-site": "same-site",
            },
            referrer: "https://divar.ir/",
            referrerPolicy: "origin",
            body: null,
            method: "GET",
            mode: "cors",
            credentials: "omit",
          }
        )
          .then((res) => res.json())
          .then((res) => res);
        return data;
      };
      const titleHtml = document.querySelector(".post-page__details-text");
      const groupInfoHtml = document.querySelector(".post-detail__list-data");

      getInfo(token).then((res) => {
        console.log(res);
        const titleSection = res.sections.find(
          (el) => el.section_name === "TITLE"
        ).widgets[0].data;
        const listData = res.sections.find(
          (el) => el.section_name === "LIST_DATA"
        );
        const groupInfoRow = listData.widgets.find(
          (el) => el.widget_type === "GROUP_INFO_ROW"
        ).data.items;
        groupInfoRow.map((el) => {
          groupInfoHtml.innerHTML += `
          <div class="list-data">
                <span class="list-data__key">${el.title}</span>
                <span class="list-data__value">${el.value}</span>
              </div>
          `;
        });
        title.innerHTML += `
              <h2 class="post-detail__title">
              ${titleSection.title}
              </h2>
              <span class="post-detail__time">
                ${titleSection.subtitle}
              </span>
        `;
      });
    </script>
